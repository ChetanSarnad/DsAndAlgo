    "    \n",
    "    if number <= 1 :\n",
    "        print('return 1 for base condition')\n",
    "        return 1\n",
    "    \n",
    "    print('Factorial for number : ' , number, '* factorial of ', number - 1)\n",
    "    return number * factorial(number -1)"
